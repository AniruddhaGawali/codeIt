#include <iostream>
using namespace std;

class  New{
  private:
    double length;
  public:
    New() {
      length = 5.5;
      cout<<"This is from constructor"<<endl;
      cout << "Creating a wall." << endl;
      cout << "Length = " << length << endl;
    }
};

int main() {
  New wall1;
  return 0;
}
